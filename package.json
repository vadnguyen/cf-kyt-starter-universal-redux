{
  "name": "cf-kyt-starter-universal-redux",
  "version": "1.3.6",
  "description": "A starter kyt with React, Redux, SSR and data fetching.",
  "main": "index.js",
  "scripts": {
    "build": "kyt build --config kyt.config.production.js && sw-precache --config=sw-precache-config.js",
    "storybook": "start-storybook -p 6006 --dont-track",
    "build-storybook": "build-storybook",
    "precommit": "lint-staged",
    "lint-staged": {
      "*.js": [
        "prettier --single-quote --trailing-comma all --write \"src/**/*.js\"",
        "git add"
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cleverfranke/cf-kyt-starter-universal-redux.git"
  },
  "author": "CLEVERÂ°FRANKE",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/cleverfranke/cf-kyt-starter-universal-redux/issues"
  },
  "homepage": "https://github.com/cleverfranke/cf-kyt-starter-universal-redux#readme",
  "kyt": {
    "files": [
      ".eslintrc.json",
      "sw-precache-config.js",
      ".storybook",
      "stories",
      "kyt.config.production.js"
    ],
    "scripts": [
      "build",
      "storybook",
      "build-storybook",
      "precommit"
    ]
  },
  "dependencies": {
    "classnames": "~2.2.5",
    "compression": "~1.6.2",
    "express": "~4.15.2",
    "react": "~15.4.2",
    "react-dom": "~15.4.2",
    "react-helmet": "~5.0.2",
    "react-redux": "~5.0.3",
    "react-router": "~2.6.1",
    "react-router-redux": "~4.0.8",
    "redux": "~3.6.0",
    "redux-promise-middleware": "~4.2.0",
    "redux-thunk": "~2.2.0"
  },
  "devDependencies": {
    "kyt": "0.5.2",
    "@kadira/storybook": "~2.35.3",
    "babel-eslint": "~7.2.1",
    "babel-plugin-transform-async-to-generator": "~6.22.0",
    "babel-plugin-transform-class-properties": "~6.23.0",
    "babel-plugin-transform-object-rest-spread": "~6.23.0",
    "babel-preset-kyt-react": "~0.2.0",
    "enzyme": "~2.8.0",
    "eslint": "~3.19.0",
    "eslint-config-prettier": "~1.7.0",
    "eslint-plugin-prettier": "~2.0.1",
    "husky": "~0.13.3",
    "lint-staged": "~3.4.0",
